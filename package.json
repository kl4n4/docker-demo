{
  "name": "docker-demo",
  "version": "0.3.0",
  "description": "docker-demo",
  "author": "",
  "private": true,
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "lint": "tslint -c tslint.json \"src/**/*.ts\"",
    "start": "node lib/index.js",
    "start:watch": "nodemon lib/index.js",
    "build": "npm run lint && echo Using TypeScript && tsc --version && tsc --pretty -p .",
    "build:docker": "npm run build && docker build -t updatemi/docker-demo:0.3.0 .",
    "push:docker": "docker push updatemi/docker-demo:0.3.0",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "restify": "^7.6.0"
  },
  "devDependencies": {
    "@types/node": "^10.12.18",
    "@types/restify": "^7.2.7",
    "nodemon": "^1.18.9",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
